	BED_MESH_CLEAR
	
	G90
	
	{% if printer.heater_bed.temperature < (BED_TEMP * 0.95) %}
	
	M140 S{BED_TEMP}
	
	G28
	
	G0 X{printer.toolhead.axis_minimum.x + 10} Y{printer.toolhead.axis_minimum.y + 10} Z5 F{250 * 60}
	
	{% endif %}
	
	M190 S{BED_TEMP}
	
	G28
	
	BED_MESH_CALIBRATE
	
	M104 S{HOTEND_TEMP}
	
	SMART_PARK
	
	M109 S{HOTEND_TEMP}
	
	LINE_PURGE
	
	SKEW_PROFILE LOAD=my_skew_profile

